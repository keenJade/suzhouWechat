.jrms_box {
  height: 80rpx;
  line-height: 80rpx;
  color:#fff;
	background-color: #c6c3f0;
}

.jrms_box swiper {
  height: 340rpx;
}

.jrms_box .jrms_hd {
  display: flex;
  width:100%;
  justify-content: space-between;
  padding: 0 30rpx;
  align-items: center;
}

.jrms_box .jrms_hd .left {
  display: flex;
  font-weight: 600;
  align-items: center;
}

.jrms_box .jrms_hd .more {
  font-size: 26rpx;
  color: #666;
  position: relative;
}

.jrms_box .jrms_hd .more::after {
  content: "";
  display: inline-block;
  position: relative;
  top: -3rpx;
  width: 10rpx;
  height: 10rpx;
  border-style: solid;
  border-width: 2rpx;
  border-color: #333 #333 transparent transparent;
  transform: rotate(45deg);
  margin-left: 10rpx;
}
.countdown{
   margin-left: 8rpx;
}
.countdown text{
  width: 34px;
	height: 40px;
  color: #c6c3f0;
	background-color: #ffffff;
}
.pagetop .swiper-box swiper {
  height: 646rpx;
}

.pagetop .swiper-box .wx-swiper-dot {
  width: 10rpx;
  display: inline-flex;
  height: 10rpx;
  justify-content: space-between;
  border-radius: 0;
  position: relative;
  top: 0;
  z-index: 8;
}

.pagetop  .swiper-box .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 0;
}

.pagetop .swiper-box .wx-swiper-dot-active {
  width: 28rpx;
}

.pagetop .swiper-box .wx-swiper-dot-active::before {
  background: rgba(255, 255, 255, 1);
  border-radius: 0;
}

