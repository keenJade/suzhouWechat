.model {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  top: 0;
  z-index: 999;
  transition: all 0.3s;
  transform: translateY(100%);
}

.model_show{
  transform: translateY(0);
}
.modelBg {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 889;
  background: rgba(0, 0, 0, 0.6);
}

