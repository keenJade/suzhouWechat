.pagetop {
  background-color: #fff;
  padding-bottom: 10rpx;
   padding: 0 30rpx;
   background-size: 100% 290rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
}
.pagetop .search_bar {
  display: flex;
  height: 60rpx;
  align-items: center;
  padding:20rpx 0;
}

.pagetop .search_bar .input_box {
  flex: 1;
  height: 60rpx;
  border-radius: 30rpx;
  background: rgba(244, 244, 244, 1);
}

.pagetop .search_bar .input_box image {
  width: 30rpx;
  height: 30rpx;
  margin: 0 20rpx 0 40rpx;
}

.pagetop .swiper-box {
  position: relative;
	background: 1rpx solid #d5b6b4;
}
.pagetop .swiper-box swiper{
  width:690rpx;
  height: 280rpx;
}
.pagetop .slide-image {
  width: 100%;
  height:100%;
  border-radius: 20rpx;
}

.pagetop .swiper-box .wx-swiper-dot {
  width: 10rpx;
  display: inline-flex;
  height: 10rpx;
  justify-content: space-between;
  border-radius: 0;
  position: relative;
  top: 0;
  z-index: 8;
}
.sw_item{
  width: 344rpx;
}
.pagetop  .swiper-box .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 0;
}

.pagetop .swiper-box .wx-swiper-dot-active {
  width: 28rpx;
}

.pagetop .swiper-box .wx-swiper-dot-active::before {
  background: rgba(255, 255, 255, 1);
  border-radius: 0;
}
