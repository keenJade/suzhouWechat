/* pages/orderDetails/orderDetails.wxss */
.top{
  display: flex;
  align-items: center;
  height: 100rpx;
  padding: 0 30rpx;
  background-color: #fff;
  position: relative;
}
.top image{
  width: 36rpx;
  height: 36rpx;
}
.top text{
  margin-left: 10rpx;
}
.userBox{
  display: flex;
  flex-direction: column;
  height:280rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 4rpx 40rpx 0px rgba(114,114,114,0.13);
  margin: 20rpx;
  padding: 24rpx 30rpx 36rpx; 
}
.userBox .userinfoBox{
 display: flex;
 justify-content: space-between;
}
.userBox .userSign{
  width: 60rpx;
  text-align: center;
  height: 32rpx;
  line-height: 28rpx;
}
.userBox .userinfo {
  display: flex;
}
.userBox .userinfo image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.userBox .userAddress{
  display: flex;
  align-items: center;
  margin-top: auto;
}
.userBox .userAddress image{
  width: 30rpx;
  height: 34rpx;
  margin-right: 10rpx;
}
.userBox .info{
  display: flex;
  flex-direction: column;
}
.userBox .info view{
  margin-bottom: 6rpx;
}
.userBox .info text{
  margin-top: 4rpx;
}
.myInfo{
display: flex;
flex-direction: column;
height:200rpx;
background:rgba(255,255,255,1);
box-shadow:0px 4rpx 40rpx 0px rgba(114,114,114,0.13);
margin: 0 20rpx 20rpx;
padding: 30rpx 30rpx 36rpx;
position: relative;
justify-content: space-between;
}
.myInfo image{
   left: 0;
   width: 100%;
   height: 4rpx;
   position: absolute;
   bottom: 0;
}
.orderInfo{
  margin: 20rpx;
}
.weui-cells::after,.weui-cells::before,.weui-cell::before{
  display: none;
}
.goodsinfoBox{
  display: flex;
  padding: 30rpx;
}
.goodsinfoBox image{
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
}
.goodsinfoBox .goodsinfo{
  width: 510rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between
}
.goodsinfoBox .goodsinfo .goodsType{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 6rpx;
}
.goodsinfoBox .money{
  display: flex;
  align-items: center;
  margin-top: auto;
  height: 30rpx;
}
.goodsinfoBox .money view{
  margin-left: auto;
}
.orderCode{
  background-color: #fff;
  margin:20rpx; 
}
.orderCode .title{
  display: flex;
  align-items: center;
  height: 100rpx;
  padding-left: 30rpx;
}
.orderCode .codeBox{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 20rpx 0 70rpx;
  border-bottom: 4rpx dashed #eee;
}
.orderCode .codeBox image{
  width: 280rpx;
  height: 280rpx;
  margin-bottom: 30rpx;
}
.orderCode .codeBox .getGoodsImg{
  width: 266rpx;
  height: 140rpx;
}
.orderCode .user{
  padding: 40rpx 30rpx;
  display: flex;
  flex-direction: column;
  }
.b-top {
  height:200rpx;
  background-size: 100% 200rpx;
  display: flex;
  flex-flow: column;
  color: #fff;
  padding-left: 40rpx;
  justify-content: center;
  background-image: url('data:image/png;base64,');

}
.img1{
  width: 43rpx;
  height: 38rpx;
  vertical-align: sub;
}
.bg-1 {
  width: 100%;
  height: 4rpx;
}
.b-address {
  padding-bottom: 45rpx;
  padding-top: 45rpx;

}
.orders-box .pro-cell {
  -webkit-box-align: inherit;
  -webkit-align-items: inherit;
  align-items: inherit;
}

.pro-box {
  position: relative;
}

.pro-box::before {
  content: " ";
  position: absolute;
  left: 15px;
  top: 0;
  right: 0;
  height: 1px;
  border-bottom: 1rpx solid #e6e6e6;
  color: #e6e6e6;
  -webkit-transform-origin: 100% 0;
  transform-origin: 100% 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.pro-box .pro-cell::before {
  content: none;
}

.orders-box .pro-cell .weui-cell__ft {
  padding-left: 20rpx;
}

.proimg {
  width: 140rpx;
  height: 140rpx;
  margin-right: 18rpx;
}

.pro-title {
  width: 530rpx;
}
.hj-cell{
  height: 100rpx;
	background-color: #fafafa;
  line-height: 100rpx;
  box-sizing: border-box;
  margin-left: 15px;
  margin-right: 15px;
}
.pro-money {
  font-size: 28rpx;
  color: #3d3d3d;
  font-weight: 600;
}
.fixed-box {
  position: fixed;
  padding: 20rpx 30rpx;
  background-color: #ffffff;
  bottom:0px;
  left: 0;
  right: 0;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.fixed-box .btn1{
  width:160rpx;
  height:56rpx;
  background:rgba(255,255,255,1);
  border:1px solid rgba(229,229,229,1);
  text-align: center;
  line-height: 56rpx;
  font-size: 28rpx;
  font-weight: bold;
  margin-left: 10rpx;
}
.fixed-box .btn1.red{
 color: #c6c3f0;
border: solid 1px #c6c3f0;
}

.default{
  background-repeat: no-repeat;
  background-position: left bottom;
  background-size: 100% 4rpx;
  background-image: url(data:image/png;base64,)
}


.pt40{
  padding-top: 40rpx;
}
